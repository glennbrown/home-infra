{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabledManagers": ["ansible", "docker-compose", "dockerfile", "pip_requirements"],
  "dependencyDashboard": true,
  "prConcurrentLimit": 0,
  "ansible": {
    "fileMatch": [
      "^ansible/group_vars/.*\\.ya?ml$",
      "^requirements\\.ya?ml$"
    ]
  },
  "pip_requirements": {
    "fileMatch": [
      "^requirements\\.txt$"
    ]
  }
}